<sentinel-form title="queue:add.title">
  <fieldset class="form-group">
    <sentinel-text focus="true" label="uri" {(value)}="uri" required="true" {errors}="errors" error-attribute="uri"/>
    <sentinel-submit-button {click}="@add" text="add" />
    <sentinel-button {click}="@close" text="close" />
  </fieldset>
</sentinel-form>
