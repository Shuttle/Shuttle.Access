{
  "name": "access",
  "version": "1.0.0",
  "description": "Identity and access control.",
  "steal": {
    "main": "main.js",
    "directories": {
      "lib": "src"
    },
    "npmAlgorithm": "flat",
    "plugins": [
      "steal-stache",
      "steal-css"
    ],
    "meta": {
      "popper.js": {
        "format": "global",
        "deps": [
          "jquery"
        ]
      },
      "bootstrap": {
        "deps": [
          "jquery",
          "popper.js"
        ]
      }
    },
    "map": {
      "popper.js": "popper.js/dist/umd/popper",
      "bootstrap": "bootstrap/dist/js/bootstrap"
    },
    "serviceBaseURL": "http://localhost:5599/api/",
    "debug": true,
    "localeVersion": "1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "steal-tools bundle --deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuttle/Shuttle.Access.git"
  },
  "author": "Eben Roux",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Shuttle/Shuttle.Access/issues"
  },
  "homepage": "https://github.com/Shuttle/Shuttle.Access#readme",
  "dependencies": {
    "bootstrap": "^4.1.1",
    "can-component": "^4.1.0",
    "can-compute": "^4.0.3",
    "can-connect": "^2.0.1",
    "can-define": "^2.3.1",
    "can-define-validate-validatejs": "^1.0.0",
    "can-route": "^4.0.1",
    "can-route-pushstate": "^4.0.5",
    "can-set": "^1.5.1",
    "can-stache": "^4.5.0",
    "can-stache-bindings": "^4.2.2",
    "can-stache-route-helpers": "^0.1.4",
    "can-util": "^3.11.6",
    "font-awesome": "^4.7.0",
    "i18next": "^11.3.2",
    "i18next-xhr-backend": "^1.5.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "shuttle-access": "^1.0.0",
    "shuttle-can-api": "^1.0.17",
    "shuttle-canstrap": "^1.0.50",
    "shuttle-guard": "^1.0.0",
    "steal": "^1.11.6",
    "steal-css": "^1.3.2",
    "steal-stache": "^4.1.0"
  },
  "devDependencies": {
    "steal-less": "^1.3.0"
  }
}
