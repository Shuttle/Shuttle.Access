{
  "name": "access",
  "version": "1.0.0",
  "description": "Identity and access control.",
  "steal": {
    "main": "main.js",
    "directories": {
      "lib": "src"
    },
    "npmAlgorithm": "flat",
    "plugins": [
      "steal-stache",
      "steal-css"
    ],
    "meta": {
      "popper.js": {
        "format": "global",
        "deps": [
          "jquery"
        ]
      },
      "bootstrap": {
        "deps": [
          "jquery",
          "popper.js"
        ]
      }
    },
    "map": {
      "popper.js": "popper.js/dist/umd/popper",
      "bootstrap": "bootstrap/dist/js/bootstrap"
    },
    "serviceBaseURL": "http://localhost:5599/api/",
    "debug": true,
    "localeVersion": "1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuttle/Shuttle.Access.git"
  },
  "author": "Eben Roux",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Shuttle/Shuttle.Access/issues"
  },
  "homepage": "https://github.com/Shuttle/Shuttle.Access#readme",
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "can-component": "^3.3.7",
    "can-compute": "^3.3.10",
    "can-connect": "^1.5.12",
    "can-define": "^1.5.4",
    "can-define-validate-validatejs": "^0.1.3",
    "can-route": "^3.3.1",
    "can-route-pushstate": "^3.2.1",
    "can-set": "^1.3.3",
    "can-stache": "^3.14.4",
    "can-stache-bindings": "^3.11.5",
    "font-awesome": "^4.7.0",
    "i18next": "^7.2.3",
    "i18next-xhr-backend": "^1.4.1",
    "popper.js": "^1.13.0",
    "shuttle-can-api": "^1.0.1",
    "shuttle-canstrap": "^1.0.21",
    "steal": "^1.6.2",
    "steal-css": "^1.3.1",
    "steal-stache": "^3.1.3"
  }
}
