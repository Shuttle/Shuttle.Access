{
  "Logging": {
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Urls": "http://localhost:5511",
  "ConnectionStrings": {
    "azure": "UseDevelopmentStorage=true",
    "Access": "Data Source=.;Initial Catalog=Access;user id=sa;password=Pass!000"
  },
  "Shuttle": {
    "ServiceBus": {
      "Inbox": {
        "WorkQueueUri": "azuremq://azure/access-webapi-inbox-work",
        "DeferredQueueUri": "azuremq://azure/access-webapi-inbox-deferred",
        "ErrorQueueUri": "azuremq://azure/shuttle-error",
        "ThreadCount": 1,
        "DurationToIgnoreOnFailure": [
          "00:00:05",
          "00:00:10",
          "00:00:30"
        ]
      },
      "MessageRoutes": [
        {
          "Uri": "azuremq://azure/access-server-inbox-work",
          "Specifications": [
            {
              "Name": "StartsWith",
              "Value": "Shuttle.Access.Messages"
            }
          ]
        }
      ]
    } 
  }
}
